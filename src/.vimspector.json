{
  "configurations": {
    "<name>: Launch": {
      "adapter": "debugpy",
      "configuration": {
        "name": "<name>: Launch",
        "type": "python",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "python": "/opt/conda-envs/ChatTester/bin/python",
        "stopOnEntry": true,
        "console": "externalTerminal",
        "debugOptions": [],
        "program": "${file}",
		"env": {
          "PYTHONPATH": "/root/TestGPT_ASE/src:/opt/conda-envs/ChatTester/bin/python"
        }
      }
    }
  }
}
